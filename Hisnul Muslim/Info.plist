	<false/>
	<key>NSPrivacyAccessedAPITypes</key>
	<array>
		<dict>
			<key>API</key>
			<string>NSUserDefaults</string>
			<key>Reason</key>
			<string>We use UserDefaults to store user preferences and settings.</string>
